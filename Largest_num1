import java.util.*;
class Main {
    public static void main(String[] args) {
        System.out.println("Try programiz.pro");
        int a[]={5,2,7,1,8,0};
        int max=-10000;
        //int max=a[0];
        for(int i=0;i<a.length;i++)
        {
            if(max<a[i])
                max=a[i];
        }
        System.out.println(max);
        
    }
}

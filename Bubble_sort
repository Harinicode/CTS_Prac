class Main {
    public static void main(String[] args) {
        int a[]={5,2,7,1,8,0};
        int temp=0;
        for(int i=0;i<a.length-1;i++)
        for (int j=0;j<a.length-1-i;j++)
            if(a[j]>a[j+1])
            {
                temp=a[j+1];
                a[j+1]=a[j];
                a[j]=temp;
            }
        for(int k=0;k<a.length;k++)
            System.out.print(a[k]+" ");
    }
}
